<aura:component implements="lightning:availableForFlowScreens" access="global">
    <aura:attribute name="starter_option" type="String" access="global" />
    
    <div class="card_wrapper">
        <lightning:layout verticalAlign="center" horizontalAlign="center" pullToBoundary="small">
            <lightning:layoutItem flexibility="auto" padding="around-small">
                <img aura:id="annuity_start" src="{!$Resource.home_button}" onmouseover="{!c.onMouseOver}" onmouseout="{!c.onMouseOut}" onclick="{!c.onSelect}" data-value="annuity_start"/>
            </lightning:layoutItem>
            <lightning:layoutItem flexibility="auto" padding="around-small">
                <img aura:id="life_start" src="{!$Resource.auto_button}" onmouseover="{!c.onMouseOver}" onmouseout="{!c.onMouseOut}" onclick="{!c.onSelect}" data-value="life_start"/>
            </lightning:layoutItem>
        </lightning:layout>
    </div>
</aura:component>